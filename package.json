    "@types/graphql-upload": "^8.0.11",
    "apollo-server": "3.6.2",
    "apollo-server-express": "3.6.2",
    "aws-sdk": "^2.1157.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "graphql": "^16.6.0",
