  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "deploy": "pm2 start --watch --ignore-watch=\"node_modules\" --name \"POSTA-API\" dist/src/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeders/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate --config ormconfig.json --name",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run --config ormconfig.json",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert --config ormconfig.json",
    "migration:delete": "rm -rf ./src/database/migrations/*"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.9",
    "@nestjs/common": "^8.0.0",
